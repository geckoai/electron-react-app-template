{
  "name": "electron-react-template",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main/main.js",
  "scripts": {
    "start": "electron-react-scripts start",
    "swagger": "electron-react-scripts swagger-generator",
    "build": "electron-react-scripts build && electron-builder",
    "electron-builder": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "crowdin:upload": "crowdin upload sources --auto-update --config ./crowdin.yaml",
    "crowdin:download": "crowdin download --verbose --config ./crowdin.yaml"
  },
  "build": {
    "productName": "Electron app",
    "appId": "org.electron.app.com",
    "copyright": "YourName",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "multiLanguageInstaller": true,
      "menuCategory": true,
      "license": "./License.txt"
    },
    "dmg": {
      "icon": "resources/icons/icon.icns",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "pkg": {
      "license": "./License.txt"
    },
    "files": [
      "dist/**/*"
    ],
    "mac": {
      "icon": "resources/icons/icon.icns",
      "category": "public.app-category.business"
    },
    "linux": {
      "icon": "resources/icons"
    },
    "win": {
      "icon": "resources/icons/icon.ico"
    }
  },
  "private": true,
  "devDependencies": {
    "@types/node": "20.16.5",
    "@crowdin/cli": "^3.19.4",
    "@geckoai/electron-react-scripts": "1.0.3",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.22",
    "@types/validator": "^13.11.9",
    "postcss-pxtorem": "5.1.1",
    "babel-plugin-import": "^1.13.3",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "prettier": "^2.6.2",
    "web-vitals": "^2.1.4",
    "electron-builder": "22.13.1",
    "electron": "23.3.13"
  },
  "dependencies": {
    "@geckoai/class-transformer": "^1.0.0",
    "@packages/i18n": "workspace:*",
    "@packages/router": "workspace:*",
    "@packages/components": "workspace:*",
    "@packages/hooks": "workspace:*",
    "@packages/utils": "workspace:*",
    "crypto-js": "^4.2.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "recoil": "^0.7.7",
    "typescript": "4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "author": "Ranyunlong",
  "license": "ISC"
}
